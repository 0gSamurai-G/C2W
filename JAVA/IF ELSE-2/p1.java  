import java.util.*;

class Range{

        public static void main(String[] args){

                Scanner scan=new Scanner(System.in);
                System.out.println("Enter a number:");                int num=scan.nextInt();
                if(num>=1 && num<=1000){
                                                                              System.out.println(num+" is in
 range of 1-1000");
                }else{                                                                                                              System.out.println(num+" is no
t in range of 1-1000");
                }

        }
}